{
  "name": "web-dissect",
  "version": "1.0.2",
  "description": "Easily grab any information from a webpage",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "test": "mocha --require ts-node/register",
    "preview": "npm run build && npm run test",
    "prettier:check": "prettier --check src/**",
    "prettier:fix": "prettier --write src/**",
    "lint": "eslint src/**",
    "clean": "rm -rf ./dist",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json",
    "prepare": "husky"
  },
  "files": [
    "dist"
  ],
  "author": "Grayseon",
  "license": "ISC",
  "dependencies": {
    "cheerio": "^1.0.0",
    "ky": "^1.7.2",
    "zod": "^3.23.8"
  },
  "keywords": [
    "web",
    "scrape",
    "scraper",
    "cheerio",
    "selector",
    "website"
  ],
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/chai": "^5.0.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "chai": "^5.1.2",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  },
  "lint-staged": {
    "src/**": [
      "npm run lint",
      "npm run prettier:fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Grayseon/dissect.git"
  },
  "bugs": {
    "url": "https://github.com/Grayseon/dissect/issues"
  },
  "types": "dist/types/cjs/index.d.ts"
}
